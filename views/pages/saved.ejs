<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <header>
        <h1>favorites items</h1>

        <ul>
            <li>
                <a href="/">home page˝</a>
            </li>
        </ul>
    </header>

    <main>
        <section>
            <%data.forEach(item=>{%>
                <div>
                    <p>
                        <%= item.name %>
                    </p>
                    <img src="<%=item.image_link %>" alt="">
                    <p>
                        <%= item.price %>
                    </p>
                    <p>
                        <%= item.description %>
                    </p>
                    <a href="/saved/<$=item.id%>"><button>more details</button></a>
                </div>
                <$})%>
             </section>
    </main>

    <footer>

    </footer>
</body>

</html>